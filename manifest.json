{
        "manifest_version": 3,
        "name": "Phishing Detector",
        "version": "1.0",
        "description": "Detect phishing links in emails and websites.",
        "permissions": ["tabs", "storage", "activeTab"],
        "background": {
            "service_worker": "background.js"
        },
        "action": {
            "default_popup": "popup.html",
            "default_icon": {
                "16": "icon.png",
                "48": "icon.png",
                "128": "icon.png"
            }
        },
        "host_permissions": ["<all_urls>"]
    }
    